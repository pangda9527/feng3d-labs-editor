{
  "name": "feng3d-editor",
  "version": "0.1.0",
  "description": "feng3d引擎编辑器\r 编辑器UI基于[egret](https://www.egret.com/)开发。",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "bundle": "dist/index.js",
  "types": "dist/index.d.ts",
  "namespace": "editor",
  "author": "feng",
  "license": "MIT",
  "scripts": {
    "start": "npm run watch",
    "clean": "rimraf \"{lib,dist}\"",
    "build": "rollup -c",
    "build:prod": "cross-env NODE_ENV=production rollup -c",
    "watch": "rollup -cw",
    "server": "node server/index.js",
    "refreshTemplate": "node scripts/refreshTemplate.js"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/feng3d/editor.git"
  },
  "keywords": [
    "editor",
    "feng3d"
  ],
  "files": [
    "out",
    "libs",
    "native",
    "projects",
    "resource",
    "codeeditor.html",
    "codeeditor.js",
    "favicon.ico",
    "index.html",
    "index.js",
    "run.html",
    "run.js"
  ],
  "dependencies": {
    "feng3d": "0.5.0",
    "@feng3d/cannon": "0.5.0",
    "@feng3d/cannon-plugin": "0.5.0",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "8.12.0",
    "rimraf": "3.0.2",
    "rollup": "2.70.1",
    "rollup-plugin-buble": "0.19.8",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-dts": "4.2.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-string": "3.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript": "1.0.1",
    "fs-extra": "^10.1.0",
    "typescript": "^4.7.4"
  }
}