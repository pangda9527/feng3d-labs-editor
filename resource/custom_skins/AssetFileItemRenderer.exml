<?xml version="1.0" encoding="utf-8"?>
<e:Skin class="AssetFileItemRenderer" width="100" height="100" xmlns:e="http://ns.egret.com/eui"
        xmlns:w="http://ns.egret.com/wing" xmlns:ns1="*">
	<e:Rect width="100%" height="100%" fillColor="0xffff00" alpha="0" />
	<e:Rect width="100%" height="100%" fillColor="0x3db0df" visible="{itemSelected}" />
	<e:Image id="icon" width="64" height="64" anchorOffsetX="0" anchorOffsetY="0" horizontalCenter="0" top="10"
	         source="{data.image}" />
	<e:Label size="12" scaleX="1" scaleY="1" textColor="0x000000" textAlign="center" width="90" x="5" bottom="4" text="{data.label}"/>
	<e:Label size="12" scaleX="1" scaleY="1" textAlign="right" bottom="25" text="{data.asset.assetType}" textColor="0x7aff00" background="true" backgroundColor="0x000000" right="18" maxWidth="64"/>
</e:Skin>